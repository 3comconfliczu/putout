{
    "printer": "putout",
    "match": {
        "README.md": {
            "declare": "off",
            "regexp/apply-literal-notation": "off"
        },
        "*.spec.js": {
            "putout/create-test": ["on", {
                "add": [
                    ["printer", "putout"]
                ]
            }]
        }
    }
}
