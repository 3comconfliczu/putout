{
    "rules": {
        "putout": "on",
        "declare-undefined-variables": ["on", {
            "declarations": {
                "click": "import {click} from '__putout_evaluate: `./` + basename(__filename).replace(`.spec.mjs`, `.js`)'",
                "mouseOver": "import {mouseOver} from '__putout_evaluate: `./` + basename(__filename).replace(`.spec.mjs`, `.js`)'"
            }
        }]
    },
    "match": {
        "*.js": {
            "convert-esm-to-commonjs": "on"
        },
        "eslint-fixture": {
            "remove-empty": "off",
            "remove-unused-variables": "off",
            "remove-unused-expressions": "off",
            "remove-unused-for-of-variables": "off",
            "remove-skip": "off",
            "remove-only": "off",
            "remove-console": "off",
            "remove-debugger": "off",
            "convert-for-to-for-of": "off"
        }
    }
}
