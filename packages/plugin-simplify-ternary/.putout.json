{
    "printer": "putout",
    "rules": {
        "putout/create-test": ["on", {
            "add": [
                ["printer", "putout"]
            ]
        }]
    },
    "match": {
        "*.md": {
            "simplify-ternary": "off",
            "remove-useless-spread/object": "off",
            "putout/declare": "off"
        }
    }
}
