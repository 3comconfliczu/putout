{
  "name": "putout",
  "version": "2.26.0",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "the pluggable code transformer",
  "homepage": "http://github.com/coderaiser/putout",
  "main": "lib/putout.js",
  "bin": {
    "putout": "bin/putout.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/putout.git"
  },
  "scripts": {
    "test": "tape 'test/*.js'",
    "test:diff": "npm test | tap-diff",
    "watch:test": "nodemon -w lib -w test -x \"npm test\"",
    "watch:example": "nodemon -w lib -w test -x \"node remove-unused\"",
    "lint:lib": "eslint lib test --ignore-pattern test/fixture",
    "lint:bin": "eslint bin -c .eslintrc.bin",
    "lint": "redrun lint:*",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@babel/traverse": "^7.1.6",
    "@babel/types": "^7.2.0",
    "align-spaces": "^1.0.0",
    "chalk": "^2.4.1",
    "cherow": "^1.6.9",
    "deepmerge": "^3.0.0",
    "estree-to-babel": "^1.0.1",
    "find-up": "^3.0.0",
    "glob": "^7.1.3",
    "recast": "^0.16.1",
    "rendy": "^2.0.0",
    "table": "^5.1.1",
    "try-catch": "^2.0.0",
    "yargs-parser": "^11.1.1"
  },
  "keywords": [
    "ast",
    "lint",
    "variable",
    "unused"
  ],
  "devDependencies": {
    "@cloudcmd/stub": "^2.3.1",
    "coveralls": "^3.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-tape": "^1.1.0",
    "just-camel-case": "^4.0.2",
    "nodemon": "^1.14.12",
    "nyc": "^13.0.1",
    "redrun": "^7.0.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.8.0",
    "try-to-tape": "^1.1.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  },
  "putout": {
    "rules": {
      "rm-debugger": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
