{
  "name": "@putout/plugin-remove-unused-variables",
  "version": "1.10.0",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "putout plugin adds ability to find and remove unused variables",
  "homepage": "http://github.com/coderaiser/putout",
  "main": "lib/remove-unused-variables.js",
  "release": false,
  "tag": false,
  "changelog": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/putout.git"
  },
  "scripts": {
    "test": "tape 'test/*.js'",
    "watch:test": "nodemon -w lib -w test -x \"npm test\"",
    "lint:eslint": "eslint lib test --ignore-pattern test/fixture",
    "lint:eslint:fix": "redrun lint:eslint -- --fix",
    "lint": "redrun transform lint:eslint",
    "lint:fix": "redrun transform:fix lint:eslint:fix",
    "transform": "putout lib test",
    "transform:fix": "putout lib test --fix",
    "coverage": "nyc npm test"
  },
  "dependencies": {},
  "keywords": [
    "putout",
    "putout-plugin",
    "plugin",
    "remove",
    "unused",
    "variables"
  ],
  "devDependencies": {
    "@babel/template": "^7.2.2",
    "@putout/eslint-config": "^1.1.0",
    "@putout/test": "^1.3.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-node": "^8.0.0",
    "just-camel-case": "^4.0.2",
    "nodemon": "^1.18.9",
    "nyc": "^13.0.1",
    "putout": "^3.8.0",
    "redrun": "^7.0.2",
    "supertape": "^1.2.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
