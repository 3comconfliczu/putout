{
  "name": "putout",
  "version": "3.10.0",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "the pluggable code transformer",
  "homepage": "http://github.com/coderaiser/putout",
  "main": "lib/putout.js",
  "bin": {
    "putout": "bin/putout.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/putout.git"
  },
  "scripts": {
    "test": "tape 'test/*.js'",
    "watch:test": "nodemon -w lib -w test -x \"npm test\"",
    "watch:example": "nodemon -w lib -w test -x \"node remove-unused\"",
    "lint:lib": "eslint lib test --ignore-pattern test/fixture",
    "lint:bin": "eslint --rule 'no-console:0' bin -c .eslintrc.bin",
    "lint": "redrun lint:*",
    "putout": "bin/putout.js bin lib test",
    "putout:fix": "bin/putout.js --fix bin lib test",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls || true"
  },
  "dependencies": {
    "@babel/traverse": "^7.1.6",
    "@babel/types": "^7.2.0",
    "@putout/plugin-remove-console": "^1.0.0",
    "@putout/plugin-remove-debugger": "^1.0.0",
    "@putout/plugin-remove-empty": "^1.0.0",
    "@putout/plugin-remove-empty-pattern": "^1.0.0",
    "@putout/plugin-remove-only": "^1.0.0",
    "@putout/plugin-remove-process-exit": "^1.0.0",
    "@putout/plugin-remove-skip": "^1.0.0",
    "@putout/plugin-remove-unused-variables": "^1.0.0",
    "@putout/plugin-split-variable-declarations": "^1.0.0",
    "align-spaces": "^1.0.0",
    "array-union": "^1.0.2",
    "chalk": "^2.4.1",
    "deepmerge": "^3.0.0",
    "espree": "^5.0.0",
    "estree-to-babel": "^1.0.1",
    "find-up": "^3.0.0",
    "glob": "^7.1.3",
    "ignore": "^5.0.4",
    "is-relative": "^1.0.0",
    "once": "^1.4.0",
    "recast": "^0.17.1",
    "table": "^5.1.1",
    "try-catch": "^2.0.0",
    "yargs-parser": "^11.1.0"
  },
  "keywords": [
    "ast",
    "lint",
    "variable",
    "unused"
  ],
  "devDependencies": {
    "@cloudcmd/stub": "^2.3.1",
    "@putout/eslint-config": "^1.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-node": "^8.0.1",
    "just-camel-case": "^4.0.2",
    "lerna": "^3.8.5",
    "mock-require": "^3.0.2",
    "nodemon": "^1.14.12",
    "nyc": "^13.0.1",
    "redrun": "^7.0.2",
    "supertape": "^1.0.3"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
