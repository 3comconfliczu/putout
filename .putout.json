{
    "match": {
        "**/packages/{test,engine-processor,{plugin,processor}-*}": {
            "putout": "on"
        },
        "**/packages/plugin-*/*.md{json}": {
            "putout-config": "on"
        }
    },
    "rules": {
        "remove-useless-spread/object": ["on", {
            "exclude": "module.exports.rules = __"
        }]
    },
    "ignore": [
        ".husky"
    ]
}
